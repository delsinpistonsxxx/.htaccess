
#DON'T SHOW DIRECTORY LISTINGS
Options -Indexes

#FOLLOW SYMBOLIC LINKS
Options +FollowSymLinks

#SET DEFAULT HANDLER
DirectoryIndex index.php index.html index.htm

RewriteEngine on

#WWW REWRITE
RewriteCond %{HTTP_HOST} ^ohiopuppride.dog$ [NC]
RewriteRule ^(.*)$ http://www.ohiopuppride.dog/$1 [L,R=301]

#REDIRECT OLD PAGE
RewriteCond %{HTTP_HOST} ^www.ohiopuppride.dog$
RewriteRule ^old-page.html$ http://www.ohiopuppride.dog/new-page [R=301,L]

#ERROR 404 PAGE
ErrorDocument 404 /404.html